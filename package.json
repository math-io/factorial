{
  "name": "math-factorial",
  "version": "0.0.0",
  "description": "Computes a factorial.",
  "author": {
    "name": "Philipp Burckhardt",
    "email": "pburckhardt@outlook.com"
  },
  "contributors": [
    {
      "name": "Philipp Burckhardt",
      "email": "pburckhardt@outlook.com"
    },
    {
      "name": "Athan Reines",
      "email": "kgryte@gmail.com"
    }
  ],
  "scripts": {
    "test": "if [ \"${TRAVIS}\" ]; then npm run test-ci; else npm run test-local; fi",
    "test-local": "tape \"./test/*.js\" | tap-spec",
    "test-ci": "npm run test-local && xvfb-run npm run test-browsers",
    "test-cov": "istanbul cover --dir ./reports/coverage --report lcov tape -- \"./test/*.js\"",
    "test-browsers": "browserify ./test/*.js | testling | tap-spec",
    "coverage": "istanbul cover --dir ./reports/codecov/coverage --report lcovonly tape -- \"./test/*.js\" && cat ./reports/codecov/coverage/lcov.info | codecov && rm -rf ./reports/codecov"
  },
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "git://github.com/math-io/factorial.git"
  },
  "keywords": [
    "compute.io",
    "compute-io",
    "compute",
    "computation",
    "mathematics",
    "math.io",
    "math-io",
    "math",
    "special functions",
    "factorial",
    "fact",
    "combinatorics",
    "gamma",
    "number"
  ],
  "bugs": {
    "url": "https://github.com/math-io/factorial/issues"
  },
  "dependencies": {
    "const-pinf-float64": "^1.0.0",
    "math-gamma": "^1.0.2"
  },
  "devDependencies": {
    "browserify": "13.x.x",
    "codecov": "1.x.x",
    "compute-incrspace": "^1.0.2",
    "const-ninf-float64": "^1.0.0",
    "istanbul": "^0.4.1",
    "jshint": "2.x.x",
    "jshint-stylish": "2.x.x",
    "math-abs": "^1.0.2",
    "tap-spec": "4.x.x",
    "tape": "4.x.x",
    "testling": "1.x.x"
  },
  "license": "MIT"
}
